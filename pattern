#include<stdio.h>


int main(){

char arr[10][10];


for(int i=0;i<10;i++){
    for(int j=0;j<10;j++){

        arr[i][j]='*';
    }
}



for(int i=0;i<10;i++){
    for(int j=0;j<10;j++){



if(j==0 || j==9 || i==0 || i==9 || i==5 ){
    printf("%c",arr[i][j]);
}

else printf(" ");
    }

    printf("\n");
}
    return 0;
}
